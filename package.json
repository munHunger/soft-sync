{
  "name": "soft-sync",
  "version": "1.0.0",
  "description": "sync your settings across devices",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "npm run package && build/soft-sync sync arch -d -f",
    "help": "npm run package && build/soft-sync --help",
    "package": "npm run build && pkg build/index.js --output build/soft-sync",
    "build": "tsc",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json"
  },
  "keywords": [
    "backup",
    "sync",
    "settings",
    "software"
  ],
  "author": "munHunger",
  "license": "ISC",
  "dependencies": {
    "typescript": "3.7.4",
    "js-yaml": "3.13.1",
    "yargs": "15.1.0",
    "winston": "3.2.1",
    "chalk": "3.0.0",
    "shelljs": "0.8.3",
    "sudo-fs-promise": "1.0.1",
    "class-validator": "0.11.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.3",
    "@types/js-yaml": "3.12.1",
    "@types/node": "^13.5.0",
    "@types/yargs": "15.0.1",
    "@types/sinon": "7.5.1",
    "@types/proxyquire": "1.3.28",
    "jasmine": "^3.5.0",
    "pkg": "4.4.2",
    "ts-node": "^8.5.4",
    "sinon": "8.1.1",
    "proxyquire": "2.1.3"
  }
}
